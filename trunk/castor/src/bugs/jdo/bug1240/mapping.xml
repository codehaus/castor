<?xml version="1.0" ?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
                           "http://castor.org/mapping.dtd">
<mapping>

  <class name="jdo.bug1240.SortedContainer" identity="id">
    <map-to table="sorted_container" xml="entity1" />
    <field name="id" type="integer" >
      <sql name="id" type="integer"/>
    </field>
    <field name="name" type="string">
      <sql name="name" type="char" />
    </field>
	<field name="twos" type="jdo.bug1240.SortedContainerItem" collection="sortedset" comparator="jdo.bug1240.CustomComparator">
		<sql many-key="id_1" />
	</field>
  </class>

  <class name="jdo.bug1240.SortedContainerItem" identity="id">
    <map-to table="sorted_item" xml="entity2" />
    <field name="id" type="integer" >
      <sql name="id" type="integer"/>
    </field>
    <field name="id1" type="integer" >
      <sql name="id_1" type="integer"/>
    </field>
    <field name="name" type="string">
      <sql name="name" type="char" />
    </field>
  </class>

</mapping>

