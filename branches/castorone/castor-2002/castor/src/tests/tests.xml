<harness name="castor">

  <category name="sybase" class="jdo.JDOCategory">
    <description>JDO tests for Sybase 11</description>
    <jdo database-name="sybase" configuration="sybase.xml" />
    <case class="jdo.Concurrent" /> 
    <case class="jdo.Deadlock" />
    <case class="jdo.DuplicateKey" />
    <case class="jdo.ReadOnly" />
    <case class="jdo.TypeHandling" />
    <case class="jdo.Join" />
    <case class="jdo.KeyGenGeneric" />
    <case class="jdo.KeyGenIdentity" />
    <case class="jdo.StoredProcedure" name="TC10" />
    <case class="jdo.ManyToMany" />
    <case class="jdo.Dependent" />
    <case class="jdo.DependentUpdate" />
    <case class="jdo.Persistent" />
    <case class="jdo.CacheLeakage" />
    <case class="jdo.Race" />  
  </category>

  <category name="oracle" class="jdo.JDOCategory">
    <description>JDO tests for Oracle8i</description>
    <jdo database-name="oracle" configuration="oracle.xml" />
    <case class="jdo.Concurrent" />
    <case class="jdo.Deadlock" />
    <case class="jdo.DuplicateKey" />
    <case class="jdo.ReadOnly" />
    <case class="jdo.Join" />
    <case class="jdo.TypeHandling" />
    <case class="jdo.KeyGenGeneric" />
    <case class="jdo.KeyGenSequence" />
    <case class="jdo.KeyGenReturning" />
    <case class="jdo.ManyToMany" />
    <case class="jdo.Dependent" />
    <case class="jdo.DependentUpdate" />
    <case class="jdo.Persistent" /> 
    <case class="jdo.CacheLeakage" />
    <case class="jdo.Race" />
  </category>

  <category name="xml" class="xml.XMLCategory">
    <description>Tests for XML marshalling framework</description>
    <case class="xml.TypeHandling" />
    <case class="xml.Inheritance" />
  </category>

</harness>

